shared_library("audio-dispatcher-pool") {
    public = [
        "dispatcher-thread.h",
        "dispatcher-channel.h",
    ]

    sources = [
        "dispatcher-thread.cpp",
        "debug-logging.h",
        "dispatcher-channel.cpp",
